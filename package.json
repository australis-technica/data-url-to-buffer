  "scripts": {
    "test": "jest --env=node",
    "build": "tsc -p .",
    "clean": "rimraf lib",
    "rebuild": "npm run clean && npm run build",
    "watch": "tsc -p . --watch"
  },
  "keywords": [],
  "author": "daniel@australis-technica.com",
  "license": "MIT",
  "devDependencies": {
    "jest": "22.4.3",
    "typescript": "2.9.2"
  },
  "dependencies": {
    "ts-jest": "23.0.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/))(test|spec)\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "engines": {
    "node": ">=0.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/australis-technica/data-url-to-buffer.git"
  },
  "bugs": {
